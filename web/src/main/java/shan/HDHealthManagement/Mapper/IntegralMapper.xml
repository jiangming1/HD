<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--命名空间应该是对应接口的包名+接口名 -->
<mapper namespace="shan.HDHealthManagement.Mapper.IntegralDao">
    
    <select id="getAll" resultType="shan.HDHealthManagement.po.Integral">
        select * from integral
    </select>
    
    <select id="getByPage" resultType="shan.HDHealthManagement.po.Integral">
        select i.id,u.name,i.integral from integral i,weixinUser u where i.weixinId = u.id LIMIT #{index},#{rows}
    </select>
    

</mapper>